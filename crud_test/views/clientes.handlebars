<div id="form-box">
  <h1>Cadasto de Clientes:</h1>
  <form action="/cliente/save" method="post" id="form-cadastro">
    <div>
      <label for="nome">Nome:</label>
      <input type="text" name="nome" id="nome" required/>
    </div>
    <div>
      <label for="endereco">Endereco:</label>
      <input type="text" name="endereco" id="endereco" required/>
    </div>
    <div>
      <label for="email">Email:</label>
      <input type="email" name="email" id="email" required/>
    </div>
    <div>
      <label for="dataNasc">Data de Nascimento:</label>
      <input type="date" name="dataNasc" id="data-nasc"/>
    </div>
    <div>
      <label for="dependentes">Qtd. Dependentes:</label>
      <input type="number" min="0" max="20" name="dependentes" id="dependentes"/>
    </div>
    <button type="submit">Cadastrar</button>
  </form>
</div>
<section>
<p>Lista de Clientes</p>
<table>
  <thead>
    <tr>
      <th>ID Cliente</th>
      <th>Nome</th>
      <th>Email</th>
      <th>EndereÃ§o</th>
      <th>Data Nascimento</th>
      <th>Qtd. Dependentes</th>
    </tr>
    {{#each clientes}}
    <tr>
        <td>{{this.id_cliente}}</td>
        <td>{{this.nome_cliente}}</td>
        <td>{{this.email_cliente}}</td>
        <td>{{this.endereco_cliente}}</td>
        <td>{{this.data_nascimento_cliente}}</td>
        <td>{{this.dependentes_cliente}}</td>
    </tr>
    {{/each}}
  </thead>
</table>
</section>